{
  "version": 2,
  "builds": [
    {
      "src": "packages/landing/package.json",
      "use": "@vercel/next",
      "config": {
        "installCommand": "npm install",
        "buildCommand": "npm run build",
        "outputDirectory": ".next"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/packages/landing/$1"
    }
  ]
} 